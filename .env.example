# MySQL Database Connection Settings
# These variables are used to connect to the source MySQL database for data scraping.
MYSQL__HOST=localhost
MYSQL__PORT=3306
MYSQL__USER=root
MYSQL__PASSWORD=
MYSQL__DATABASE=db_penjualan

# Pinecone Vector Database Configuration
# Get your Pinecone API key from https://app.pinecone.io/
PINECONE__API_KEY=your-pinecone-api-key
PINECONE__INDEX_URL=https://your-index.pinecone.io
PINECONE__NAMESPACE=default
PINECONE__BATCH_SIZE=100

# Google Gemini API Configuration
# Get your API key from https://makersuite.google.com/app/apikey
GEMINI__API_KEY=your-gemini-api-key
GEMINI__MODEL=gemini-1.5-flash
GEMINI__TEMPERATURE=0.7
GEMINI__MAX_OUTPUT_TOKENS=2048
GEMINI__TOP_P=0.95
GEMINI__TOP_K=40

# Application Settings
# Configuration for text processing, embeddings, and batching.
APP__EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2
APP__EMBEDDING_DIMENSION=384
APP__CHUNK_SIZE=500
APP__CHUNK_OVERLAP=50
APP__BATCH_SIZE=100

# JSON Endpoint Configuration
# Configure the API endpoint that returns JSON data
JSON_ENDPOINT__URL=https://api.example.com/data
JSON_ENDPOINT__METHOD=GET
JSON_ENDPOINT__TIMEOUT=30
